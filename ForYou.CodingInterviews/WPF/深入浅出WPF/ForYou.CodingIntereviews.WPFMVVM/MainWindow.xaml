<Window x:Class="ForYou.CodingIntereviews.WPFMVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ForYou.CodingIntereviews.WPFMVVM"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <Style x:Key="tb01" TargetType="TextBox">
            <Setter Property="Height" Value="20"></Setter>
            <Setter Property="Width" Value="100"/>
            <Setter Property="Margin" Value="10"/>
        </Style>
        <Style TargetType="TextBox" x:Key="tb02" BasedOn="{StaticResource tb01}">
            
        </Style>
    </Window.Resources>
    <Grid>
        <StackPanel>
            <TextBox Style="{StaticResource tb02}">
                
            </TextBox>
        </StackPanel>
    </Grid>
</Window>
